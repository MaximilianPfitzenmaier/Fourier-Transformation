{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {},
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react/jsx-runtime"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["warn", "single", { "allowTemplateLiterals": true }],
    "no-unused-vars": ["warn"]
  }
}
